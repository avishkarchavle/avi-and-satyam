<% layout('layouts/boilerplate')%>

    <style>
        #profile {
            background-color: white;
            border: 1.5px solid green;
        }

        #profile:hover {
            background-color: green;
            color: white;
        }
    </style>

    <body>
        <nav class="navbar navbar-light bg-light"
            style="position: sticky; padding: 0px; border-bottom: 1.0px solid green; ">

            <div class="container-fluid" style="background-color: black;">
                <div class="d-flex">
                    <a href="/students/<%=student._id%>/logout">
                        <h1 class="btn btn-outline-success" id="profile">Logout</h1>
                    </a>
                </div>

                <h3 class="navbar-brand" style="color: green; text-align: center; font-size: 50px;">Level Up!
                </h3>
                <div class="d-flex">
                    <a href="/students/<%=student._id%>/edit">
                        <h1 class="btn btn-outline-success" id="profile">Profile</h1>
                    </a>
                </div>
            </div>

        </nav>



        <nav class="navbar navbar-light bg-light" style="position: sticky; border-bottom: 1.5px solid green;">
            <div class="container-fluid" style="background-color: white; height: 50px;">
                <form class="d-flex" action="/students/<%=student._id%>/search" method="post">
                    <input class="form-control me-2" id="txt" type="search" placeholder="Search by District"
                        name="search">
                    <button class="btn btn-outline-success">Search</button>
                </form>
            </div>
        </nav>

        <%if(teachers.length){%>
            <h1 class="text-center" style="color: green">Teachers Found!!!</h1>
            <div class="d-flex justify-content-center ml-3">
                <div class="row">
                    <%for(teacher of teachers){%>
                        <div class="col-sm-6 col-xs-12 card mx-3 mb-3 card text-white bg-warning  mb-3"
                            style="max-width: 18rem;" style=" display:inline-block;">
                            <div class=" card-header ">Subject:
                                <%=teacher.subject%>
                            </div>
                            <div class=" card-body ">
                                <h5 class=" card-title ">Name:
                                    <%=teacher.firstName%>
                                        <%=teacher.lastName%>
                                </h5>
                                <p class=" card-text ">
                                <ul>

                                    <li>
                                        Address :
                                        <%=teacher.address%>
                                    </li>

                                    <li>
                                        Contact :
                                        <%=teacher.phone%>
                                    </li>

                                    <li>
                                        Experience :
                                        <%=teacher.experience%>
                                    </li>

                                    <li>
                                        Additional Information :
                                        <%=teacher.additionalInfo%>
                                    </li>
                                </ul>

                                </p>
                            </div>
                        </div>
                        <%}%>
                </div>
            </div>
